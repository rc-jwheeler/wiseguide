  margin-top: -0.3em;
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  border-radius: 5px;
}
div.predefined-report form.run-report input[type="submit"]:hover {
  background: #009FFB;
}

body.reports table.auto-zebra th {
