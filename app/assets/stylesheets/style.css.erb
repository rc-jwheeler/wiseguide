/* Predefined Reports */
.report-header {
  border-bottom: 1px solid #bbb;
  padding-bottom: 5px;
  margin-bottom: 5px;
}
div.predefined-report {
  position: relative;
}
div.predefined-report form.run-report {
  clear: none;
}
div.predefined-report form.edit_report {
}
input.delete-report {
  background: image-url('cross-button.png') left center no-repeat scroll;
  color: #e5004d;
  text-decoration: none;
  font-size: 11px;
  line-height: 16px;
  border: 0;
  padding: 5px 0 5px 17px;
  float: right;
  position: absolute;
  top: 5px;
  right: 10px;
}
input.delete-report:hover,
input.delete-report:active {
  color: #ff4d88;
  text-decoration: underline;
}
div.predefined-report ol {
  list-style: none;
  margin: 0 0 10px;
  float: left;
}
div.predefined-report ol li {
  float: left;
  margin-right: 20px;
}

div.predefined-report form.run-report input[type="submit"] {
  background: #0081CC;
  color: #fff;
  font-size: 15px;
  text-decoration: none;
  border: 0;
