<h1>Assessment Requests</h1>
<table>
  <tbody>
    <% unless @kase.assessment_request.nil? %>
      <tr class="even">
        <td>This case was generated from an assessment request dated <%= @kase.assessment_request.created_at.to_date.strftime('%-m/%-d/%Y') %></td>
        <td><%= link_to "Details", @kase.assessment_request, :class=>"details" %></td>
      </tr>
    <% end %> 
  </tbody>
</table>